{"version": 2, "builds": [{"src": "frontend/package.json", "use": "@vercel/static-build", "config": {"distDir": "frontend/dist"}}], "routes": [{"src": "/(.*)", "dest": "/frontend/$1"}], "env": {"VITE_API_URL": "@api_url", "VITE_WS_URL": "@ws_url"}, "headers": [{"source": "/(.*)", "headers": [{"key": "X-Content-Type-Options", "value": "nosniff"}, {"key": "X-Frame-Options", "value": "DENY"}, {"key": "X-XSS-Protection", "value": "1; mode=block"}]}]}